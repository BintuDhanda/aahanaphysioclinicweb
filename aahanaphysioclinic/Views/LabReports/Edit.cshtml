@model aahanaphysioclinic.Model.LabReport

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Edit</h1>

<h4>LabReport</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="UploadTime" class="control-label"></label>
                <input asp-for="UploadTime" class="form-control" />
                <span asp-validation-for="UploadTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ReportUrl" class="control-label"></label>
                <input asp-for="ReportUrl" class="form-control" />
                <span asp-validation-for="ReportUrl" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LabReportType" class="control-label"></label>
                <input asp-for="LabReportType" class="form-control" />
                <span asp-validation-for="LabReportType" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EncounterId" class="control-label"></label>
                <select asp-for="EncounterId" class="form-control" asp-items="ViewBag.EncounterId"></select>
                <span asp-validation-for="EncounterId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

